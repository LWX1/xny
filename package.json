  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^11.0.4",
    "electron-builder": "^22.9.1",
    "electron-packager": "^15.2.0",
    "electron-winstaller": "^4.0.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
  
}
