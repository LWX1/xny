{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-packager . HelloWorld --platform=win32 --arch=x64  --out=./out --asar --app-version=0.0.1 --overwrite --ignore=node_modules",
    "build": "electron-builder"
